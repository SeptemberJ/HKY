.TopSubmitBt{
  width: 150rpx;
  height: 30px;
  float: right;
  display: block;
  background: #3498db;
  color: #fff;
  font-size: 32rpx;
  font-weight: bold;
  text-align: center;
  line-height: 30px;
  margin-right: 20rpx;
  border-radius: 10rpx;
}
.Scene_EQList{
  width: 750rpx;
  padding: 10rpx 0;
  display: flex;
  flex-direction: column;
  align-items: space-between;
  justify-content: center;
}
.Scene_EQList_item{
  width: 750rpx;
  height: 130rpx;
  display: flex;
  justify-content: space-between;
  align-items: center; 
  background: #fff;
  border-bottom: 1px solid #efefef;
}
.Scene_EQList_item image{
  width: 80rpx;
  height: 80rpx;
  margin: 0 10rpx;
  border-radius: 50%;
  display: inline-block;
}
.Scene_EQList_item_text{
  width: calc(750rpx - 100rpx - 150rpx);
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  justify-content: center;
}
.Scene_EQList_item_text_name{
  font-size: 32rpx;
  font-weight: bold;
  margin-bottom: 8rpx;
}
.Scene_EQList_item_text_room{
  font-size: 30rpx;
  color: #999;
}
.Scene_EQList .ControlBt{
  width: 50rpx;
  height: 50rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 50%;
  margin-right: 20rpx;
  border: 4rpx solid #3498db;
}
.Scene_EQList .ControlBt image{
  width: 40rpx;
  height: 40rpx;
}