page{
  background: #efefef;
}
.HomeList{
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  justify-content: flex-start;
}
.HomeItem{
  width: calc(100% - 20rpx);
  height: 80rpx;
  padding: 10rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  background: #fff;
  border-bottom: 1px solid #efefef;
}
.HomeItem_name{
  width: calc(100% - 150rpx - 60rpx)
}
.HomeItem_role{
  width: 150rpx;
  color: #ccc;
  text-align: right;
}
